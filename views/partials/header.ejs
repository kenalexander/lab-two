        </li>
        <li class="nav-item">
          <a class="nav-link" href="/views/ken3">Ken III</a>
        </li>
      </ul>

    </nav>

    <% if ( locals.messages && messages.length > 0 ) { %>
      <div class="alert alert-danger">
        <% for ( let message of messages ) { %>
          <%= message %>
        <% } %>
      </div>
    <% } %>
    
    <div class="container">
      <header>
        <div class="jumbotron">
          <h1><center><%= title %></center></h1>
        </div>
      </header>

    </div>
  </body>
  </html>